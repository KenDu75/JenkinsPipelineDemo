pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building step !'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying step !'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing step !'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing step !'
            }
        }
    }
}
